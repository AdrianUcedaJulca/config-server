########## MYSQL #################
  jpa:
    database : MYSQL
      naming-strategy : org.hibernate.cfg.ImprovedNamingStrategy
    hibernate:
      ddl-auto: create
    show-sql: true
    generate-ddl: true
    properties:
      hibernate:
        dialect: org.hibernate.dialect.MySQL5Dialect
  datasource:
    url: jdbc:mysql://localhost:3306/idat-estudiante
    username: root
    password: admin
    driver-class-name: com.mysql.jdbc.Driver
